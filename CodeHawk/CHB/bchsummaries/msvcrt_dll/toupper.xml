<?xml version="1.0" encoding="UTF-8"?>
 <codehawk-binary-analyzer>
  <header date="08/05/2013 10:38:58"/>
  <reference href="http://pubs.opengroup.org/onlinepubs/000095399/"/>
  <libfun lib="msvcrt" name="toupper">
   <documentation>
    <desc>transliterates lowercase characters to uppercase</desc>
    <apidoc>
     <pt>int toupper (int c)</pt>
     <par name="c">
       character to be transformed. This argument is an int, the value of which must
       be representable as an unsigned char or equal to the value of the macro EOF.
       If the argument has any other value, the behavior is undefined.
     </par>
     <return>
      <rc cond="success">uppercase letter corresponding to c</rc>
     </return>
    </apidoc>
   </documentation>
   <api adj="0" cc="cdecl" name="toupper">
     <returntype>int</returntype>
     <par loc="stack" name="c" io="r" roles="none" nr="1">
       <type>int</type>
    </par>
   </api>
   <semantics>
    <preconditions/>
    <postconditions/>
    <sideeffects/>
   </semantics>
  </libfun>
